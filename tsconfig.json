{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "incremental": true,
    "rootDir": ".",
    "isolatedModules": true,
    "paths": {
      "@/components/*": [
        "./app/src/components/*"
      ],
      "@/environmentvariables": [
        "./app/src/environmentvariables.ts"
      ],
      "@/pages/*": [
        "./app/src/pages/*"
      ],
      "@/views/*": [
        "./app/src/views/*"
      ],
      "@/stores/*": [
        "./app/src/stores/*"
      ],
      "@/api/*": [
        "./app/src/api/*"
      ],
      "@/utils/*": [
        "./app/src/utils/*"
      ],
      "@/models/*": [
        "./app/src/models/*"
      ],
      "@/hooks/*": [
        "./app/src/hooks/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
}
